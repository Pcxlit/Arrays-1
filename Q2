    cout << "Enter elements: ";
    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        s.insert(x); // duplicates will be ignored
    }

    cout << "\nArray after removing duplicates (sorted): ";
    for (int val : s) {
        cout << val << " ";
