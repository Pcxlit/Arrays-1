#include<iostream>
using namespace std;

int main(){
    cout<<"enter number of elements in an array"<<endl;   //create an array
    int n;
    cin>>n;
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        cout<<"enter value of array index "<<i<<endl;
        cin>>arr[i];
    }
    
    for (int i = 0; i < n; i++)     //display an array
    {
        cout<<arr[i]<<" ";
    }
    
    for (int i = 0; i < n; i++) //find duplicates and remove them
    {
        for (int j = (i+1); j < n; )
        {
            if (arr[i]==arr[j]) //checking the duplicates
            {
                for (int k = j; k < (n-1); k++)  //replacing the duplicates
                {
                   arr[k]=arr[k+1];
                }
                n--;
            }   
            else
            {
                j++;
            }
            
        } 
    }

     for (int i = 0; i < n; i++)     //display an array
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}

